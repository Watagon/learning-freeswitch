<include>
  <extension name="public_mylua">
    <condition field="destination_number" expression="^05011112222$">
      <action application="lua" data="hello.lua"/>
    </condition>
  </extension>
  <extension name="public_mylua_test_send_fax">
    <condition field="destination_number" expression="^05033334444$">
      <action application="lua" data="send_fax.lua"/>
    </condition>
  </extension>
  <extension name="public_fax_receive_prototype">
    <condition field="destination_number" expression="^05033334445$">
      <action application="answer" />
      <action application="playback" data="silence_stream://2000"/>
      <action application="rxfax" data="/tmp/FAX-${uuid}.tif"/>
      <action application="hangup"/>
    </condition>
  </extension>
  <extension name="public_mylua_test_receive_fax">
    <condition field="destination_number" expression="^05033334446$">
      <action application="lua" data="receive_fax.lua"/>
    </condition>
  </extension>
  <extension name="public_fax_send_prototype">
    <condition field="destination_number" expression="^05033334447$">
      <action application="answer" />
      <action application="txfax" data="/tmp/artifacts/sample.tiff"/>
      <action application="hangup"/>
    </condition>
  </extension>
  <extension name="public_user2user">
    <condition field="destination_number" expression="^user2user_1001$">
      <action application="lua" data="user2user.lua"/>
    </condition>
  </extension>
  <extension name="public_user2user_bridge">
    <condition field="destination_number" expression="^user2user_1001_bridge$">
      <action application="lua" data="user2user_bridge.lua"/>
    </condition>
  </extension>
</include>

